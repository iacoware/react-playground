{
    "name": "react-playground",
    "version": "1.0.0",
    "description": "Minimal React playground",
    "repository": {
        "url": "https://github.com/iacoware/react-playground"
    },
    "main": "src/index.js",
    "scripts": {
        "clean": "rimraf dist .cache node_modules/.cache/",
        "start": "npm run clean && parcel index.html --no-autoinstall --open",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
        "typecheck": "tsc --noEmit",
        "typecheck:w": "tsc --noEmit --watch"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "rimraf": "3.0.2"
    },
    "devDependencies": {
        "@types/react": "18.0.9",
        "@types/react-dom": "18.0.4",
        "@typescript-eslint/eslint-plugin": "5.25.0",
        "eslint": "8.16.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.30.0",
        "parcel": "2.5.0",
        "prettier": "2.6.2",
        "typescript": "4.6.4"
    }
}
